<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="@dimen/signin_item_height"
    android:paddingLeft="50dp"
    tools:context=".Signin.AccountItemFragment">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/nameAccFragText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailAccFragText" />

    <TextView
        android:id="@+id/emailAccFragText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/nameAccFragText"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>